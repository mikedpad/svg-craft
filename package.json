  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/svg-craft.git"
  },
  "keywords": ["svg", "craft", "creator"],
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --format=pretty 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "build": "react-scripts build",
